{
  //托管主机
  "spiderHost": {
    //启动项
    "startupAssembly": "",
    "startupType": "",
    //超时(以秒计)
    "timeout": 5
  },

  "schedule": {
    //轮询数据库频率(单位秒)
    "queryFrequency": 5
  },

  //存储
  "storage": {
    //连接字符串
    "connectionString": "Data Source=localhost;Initial Catalog=Product_eCPM-Tiger;User Id=sa;Password=Password01!",
    //存储类型
    "type": "",
    //数据库类别 sqlserver,mysql,sqlite,oracle,etc
    "database": "",
    //数据库客户端版本
    "clientVersion": "",
    //确保数据库对象存在，为true时若数据库对象不存在则根据实体元数据创建
    "ensureDatabaseObject": true
  },

  //网络
  "network": {
    "useProxy": false,
    "protocol": "http",
    "address": "127.0.0.1"
  },

  //selenium webdriver 设置
  "webdriver": {
    //浏览器 chrome,firefox,edge,ie
    "browser": "chrome",
    //图片下载路径
    "imageDownloadPath": "d:/images",
    //webdriver执行文件路径
    "exePath": "d:/webdriver",
    //超时时间(以秒计)
    "timeout": 60,
    //进程数
    "processes": 5
  },

  //硬件资源约束
  "hardwareResourcesRestraint": {
    //cpu空闲百分比最小值
    "cpuFreePercentageMinimum": 10,
    //内存空闲百分比最小值
    "memoryFreePercentageMinimum": 10
  },

  //kafka队列设置
  "kafka": {
    //服务器
    "bootstrapServers": "localhost:5001;localhost:5002",
    "consumerGroup": "datum_collection"
  },

  //rabbitMQ设置
  "rabbitMQ": {
    "exchange": "rabbitMQ_Exchange_Master",
    "queue": "rabbitMQ_Queue_"
  },

  //订阅话题
  "topic": {
    "statisticsFail": "Topic_Statistics_Fail",
    "statisticsSuccess": "Topic_Statistics_Success"
  },

  //邮件设置
  "email": {
    "host": "",
    "port": 25,
    "from": "",
    "password": "",
    "displayName": ""
  }
}
